    /* Improved Carousel */
    .carousel-item {
      height: 90vh;
      background-size: cover;
      background-position: center;
      position: relative;
      overflow: hidden;
    }

    .carousel-item::before {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background: rgba(0, 0, 0, 0.4);
    }

    .carousel-caption {
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);
      text-align: center;
      animation: slideIn 1s ease-out;
    }

    .carousel-caption h5 {
      font-size: 3rem;
      font-weight: 700;
      color: #fff;
      text-shadow: 2px 2px 8px rgba(0, 0, 0, 0.6);
    }

    .carousel-caption p {
      font-size: 1.5rem;
      color: #fff;
      text-shadow: 2px 2px 8px rgba(0, 0, 0, 0.6);
    }

    @keyframes slideIn {
      from {
        opacity: 0;
        transform: translate(-50%, -40%);
      }
      to {
        opacity: 1;
        transform: translate(-50%, -50%);
      }
    }

    /* About Us */
