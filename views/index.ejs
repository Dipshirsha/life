  width: 960px;
  height: 100px;
  margin: 0 auto;
}

.text-copy {
  fill: none;
  stroke: white;
  stroke-dasharray: 6% 29%;
  stroke-width: 5px;
  stroke-dashoffset: 0%;
  animation: stroke-offset 5.5s infinite linear;
}

.text-copy:nth-child(1) {
  stroke: #4D163D;
  animation-delay: -1;
}

.text-copy:nth-child(2) {
  stroke: #840037;
  animation-delay: -2s;
}

.text-copy:nth-child(3) {
  stroke: #BD0034;
  animation-delay: -3s;
}

.text-copy:nth-child(4) {
  stroke: #BD0034;
  animation-delay: -4s;
}

.text-copy:nth-child(5) {
  stroke: #FDB731;
  animation-delay: -5s;
}

@keyframes stroke-offset {
  100% {
    stroke-dashoffset: -35%;
  }
}

.gallery {
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  gap: 20px;
  padding: 20px;
  background-image: url("https://4kwallpapers.com/images/walls/thumbs_3t/8324.png");


}

.image-container {

  width: calc(50% - 20px);
  max-width: 400px;
  height: 45vh;
  perspective: 1000px;
  position: relative;
  overflow: hidden;
  margin-bottom: 20px;

}

.image-container img {
  width: 100%;
  height: 45vh;
  object-fit: cover;
  transition: transform 0.5s;
}

.image-front,
.image-back {
  width: 100%;
  height: 100%;
  position: absolute;
  backface-visibility: hidden;
  transition: transform 0.5s;
}

.image-front {
  transform: rotateY(0deg);
}

.image-back {
  transform: rotateY(180deg);
  background-color: #333;
  color: white;
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  padding: 10px;
}

.image-container:hover .image-front {
  transform: rotateY(180deg);
}

.image-container:hover .image-back {
  transform: rotateY(0deg);
}

.image-container:hover img {
  transform: rotateY(180deg);
}

.event-container {
  position: relative;


}

.event-overlay {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #00000093;
  overflow: hidden;
  width: 0;
  height: 100%;
  transition: .5s ease;
  backdrop-filter: blur(3px);
  border-radius: 20px;
}

.event-container:hover .event-overlay {
  width: 100%;
}

.event-text {
  white-space: nowrap;
  color: white;
  font-size: 20px;
  position: absolute;
  overflow: hidden;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  -ms-transform: translate(-50%, -50%);

}

.event-img {
  display: block;
  width: 100%;
  height: 300px;
  border-radius: 20px;
}

.events {
  display: flex;
  gap: 20px;
  justify-content: center;
  align-items: center;
  background-image: url("https://img.freepik.com/free-vector/bokeh-lights-effect-dark-background_52683-33179.jpg?w=1060&t=st=1692638957~exp=1692639557~hmac=c4507abc3631dce769b8b827c837b6471c5523e0a772996ac0fc700e5be27678");
  padding-top: 3vh;
}

.animate-charcter {
  text-transform: uppercase;
  background-image: linear-gradient(-225deg,
      #231557 0%,
      #44107a 29%,
      #ff1361 67%,
      #fff800 100%);
  background-size: auto auto;
  background-clip: border-news;
  background-size: 200% auto;
  color: #fff;
  background-clip: text;
  /*   text-fill-color: transparent; */
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  animation: textclip 2s linear infinite;
  display: inline-block;
  font-size: 40px;
}

@keyframes textclip {
  to {
    background-position: 200% center;

  }
}

.news {
  display: flex;
  gap: 10px;
  justify-content: center;
  align-items: center;
  padding-top: 30px;
  background-image: linear-gradient(to right, rgb(92, 18, 18), rgb(6, 15, 37), rgb(6, 15, 37), rgb(89, 7, 7));
  padding-bottom: 10vh;
}

.news-container {
  position: relative;
  border: 5px solid #000000;
  color: rgb(5, 3, 3);
  box-shadow: black;
  text-shadow: wheat;
  transition: transform .9s;
 
}
.news-container:hover{
