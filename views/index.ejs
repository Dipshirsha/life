      animation-iteration-count: infinite;

      -webkit-animation-direction: alternate;
      animation-direction: alternate;
      display: flex;
      padding-top: 40px;
      padding-bottom: 40px;

    }

    @keyframes slidein {
      from {
        background-position: top;
        background-size: 3000px;
      }

      to {
        background-position: -100px 0px;
        background-size: 2750px;
      }

